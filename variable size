#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int x,y,n,s;
    cin>>x>>y;
    int* a[x];
    while(x--)
    { 
        cin>>n;
        a[s]=new int[n];
        for(int i=0;i<n;i++)
        {cin>>a[s][i];}
         
        s++;
    }
    
    while(y--)
    {
        int p,q;
        cin>>p>>q;
        cout<<a[p][q]<<"\n";
    }
    
    return 0;
}
