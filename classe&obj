#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
#include <cassert>
using namespace std;

class Student{
    private :
    int score[5],sum=0;
    public :
    void input()
    {
        for(int i=0;i<5;i++)
        {
            cin>>score[i];
        }
        
    }
    int calculateTotalScore()
    {
        for(int i=0;i<5;i++)
        sum+=score[i];
        
        return sum;
    }
};

int main() {
